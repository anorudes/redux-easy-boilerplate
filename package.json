{
  "name": "redux-easy-boilerplate",
  "engines": {
    "node": ">=5.0"
  },
  "version": "3.0.1",
  "description": "Start your project easy and fast with modern tools",
  "scripts": {
    "start": "rimraf dist && NODE_ENV=development node bin/server.js",
    "start:prod": "NODE_ENV=production node bin/server.js",
    "start-ssr": "rimraf dist && NODE_ENV=development concurrently --kill-others \"nodemon --watch app bin/server-ssr.js\" \"node bin/webpack-server.js\"",
    "start-ssr:prod": "NODE_ENV=production node bin/server-ssr.js",
    "api": "NODE_ENV=development nodemon --watch api bin/api.js",
    "api:prod": "NODE_ENV=production node bin/api.js",
    "api-debug": "NODE_ENV=development concurrently --kill-others \"nodemon --debug --watch api bin/api.js\"  \"node-inspector -p 3020\"",
    "build": "rimraf dist && NODE_ENV=production webpack --progress --verbose --display-error-details --config webpack/common.config.js",
    "build:analyze": "NODE_ENV=production webpack --json --config webpack/common.config.js | webpack-bundle-size-analyzer > logs/build-analyze.log",
    "test": "NODE_PATH=./app mocha --require ./bin/test.js 'app/**/*spec.js'",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anorudes/redux-easy-boilerplate.git"
  },
  "homepage": "http://anorudes.github.io/redux-easy-boilerplate/",
  "bugs": "https://github.com/anorudes/redux-easy-boilerplate/issues",
  "keywords": [
    "react",
    "reactjs",
    "babel6",
    "boilerplate",
    "redux",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
